{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": true
    },
    {
      "name": "GraphQL Serve .NET Core Launch (web)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build-graphQL-server",
      "program": "${workspaceFolder}/netcore-graphQL/Server/bin/Debug/net6/GraphQL.Samples.Server.dll",
      "args": [],
      "cwd": "${workspaceFolder}/netcore-graphQL/Server",
      "stopAtEntry": false,
      "internalConsoleOptions": "openOnSessionStart",
      "launchBrowser": {
        "enabled": true,
        "args": "${auto-detect-url}",
        "windows": {
          "command": "cmd.exe",
          "args": "/C start ${auto-detect-url}"
        },
        "osx": {
          "command": "open"
        },
        "linux": {
          "command": "xdg-open"
        }
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "sourceFileMap": {
        "/Views": "${workspaceFolder}/Views"
      }
    },
    {
      "name": "GraphQL .NET Core Launch (console)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build-graphQL",
      "program": "${workspaceFolder}/netcore-graphQL/hello-world/bin/Debug/net6.0/netcore-graphQL.dll",
      "args": [],
      "cwd": "${workspaceFolder}",
      "stopAtEntry": false,
      "console": "integratedTerminal"
    },
    {
      "name": "gRPC .NET Core Launch (console)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build-gRPC",
      "program": "${workspaceFolder}/netcore-gRPC/greeter/bin/Debug/net5.0/GrpcGreeter.dll",
      "args": [],
      "cwd": "${workspaceFolder}",
      "stopAtEntry": false,
      "console": "integratedTerminal"
    },
    {
      // Use IntelliSense to find out which attributes exist for C# debugging
      // Use hover for the description of the existing attributes
      // For further information visit https://github.com/OmniSharp/omnisharp-vscode/blob/master/debugger-launchjson.md
      "name": "Blazor .NET Core Launch (web)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build-blzrapp",
      // If you have changed target frameworks, make sure to update the program path.
      "program": "${workspaceFolder}/netcoreblzrapp/bin/Debug/net5.0/netcoreblzrapp.dll",
      "args": [],
      "cwd": "${workspaceFolder}/netcoreblzrapp",
      "stopAtEntry": false,
      // Enable launching a web browser when ASP.NET Core starts. For more information: https://aka.ms/VSCode-CS-LaunchJson-WebBrowser
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "sourceFileMap": {
        "/Views": "${workspaceFolder}/Views"
      }
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach"
    }
  ]
}