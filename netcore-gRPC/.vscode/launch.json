  "version": "0.2.0",
  "configurations": [
    {
      "name": "gRPC greeter .NET Core Launch (console)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build-server",
      "program": "${workspaceFolder}/greeter/bin/Debug/net5.0/GrpcGreeter.dll",
      "args": [],
      "cwd": "${workspaceFolder}",
      "stopAtEntry": false,
      "console": "internalConsole"
    },
    {
      "name": "gRPC client .NET Core Launch (console)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build-client",
      "program": "${workspaceFolder}/client/bin/Debug/net6.0/client.dll",
      "args": [],
      "cwd": "${workspaceFolder}/client",
      "console": "internalConsole",
      "stopAtEntry": false
    }
  ]
}